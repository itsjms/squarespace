		            </div>
		          {.end}
		        {.end}
		        <squarespace:block-field id="footer-blocks" columns="12" label="Footer Content" />
		      </div>
		    </div><!-- page-footer-wrapper -->
		  

		  </div>
		</div> <!-- canvas-wrapper -->

		{squarespace-footers}

	</body>
